{
  "name": "vaske-app",
  "main": "expo-router/entry",
  "version": "1.0.0",
  "scripts": {
    "start": "yarn supabase:types && yarn supabase:apply-migrations && expo start -c",
    "android": "yarn supabase:types && yarn supabase:apply-migrations && expo run:android",
    "ios": "yarn supabase:types && yarn supabase:apply-migrations && expo run:ios",
    "android:dev": "yarn supabase:types && yarn supabase:apply-migrations && eas build --profile development --platform android'",
    "ios:dev": "yarn supabase:types && yarn supabase:apply-migrations && eas build --profile development --platform ios'",
    "web": "expo start --web",
    "test": "jest --watchAll",
    "lint": "echo \"\\033[33mRunning eslint check\" &&  eslint .",
    "extract": "formatjs extract src/**/*.{ts,tsx,jsx,js} --ignore='**/*.d.ts' --out-file ./extractedTranslations/en.json",
    "supabase:types": "supabase gen types typescript --local > ./src/core/types/supabase.types.gen.ts",
    "supabase:apply-migrations": "supabase start && supabase migration up && supabase db reset",
    "supabase:sync-staging": "supabase link --project-ref tpvckrtnjhwwleuxchzu && supabase db pull && supabase migration up && supabase db reset",
    "supabase:sync-production": "supabase link --project-ref tpvckrtnjhwwleuxchzu && supabase db pull && supabase migration up && supabase db reset",
    "storybook": "yarn storybook:generate && EXPO_PUBLIC_STORYBOOK_ENABLED='true' expo start -c",
    "storybook:ios": "sb-rn-get-stories --config-path .ondevice && EXPO_PUBLIC_STORYBOOK_ENABLED='true' expo start --ios",
    "storybook:android": "sb-rn-get-stories --config-path .ondevice && EXPO_PUBLIC_STORYBOOK_ENABLED='true' expo start --android",
    "storybook:generate": "sb-rn-get-stories --config-path .ondevice",
    "storybook:watch": "sb-rn-watcher --config-path .ondevice",
    "storybook:web": "start-storybook -p 6006",
    "storybook:build": "build-storybook"
  },
  "jest": {
    "preset": "jest-expo"
  },
  "dependencies": {
    "@expo/vector-icons": "^13.0.0",
    "@react-native-async-storage/async-storage": "1.18.2",
